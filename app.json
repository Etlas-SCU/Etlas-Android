{
  "expo": {
    "name": "Etlas",
    "slug": "Etlas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/Etlas-logo.png",
    "userInterfaceStyle": "dark",
    "githubUrl": "https://github.com/Etlas-SCU/Etlas-Android",
    "splash": {
      "image": "./assets/splash/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [ "**/*" ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.etlas"
    },
    "android": {
      "androidStatusBar": {
        "translucent": true,
        "backgroundColor": "transparent",
        "barStyle": "light-content"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Etlas-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [ "android.permission.CAMERA", "android.permission.RECORD_AUDIO" ],
      "package": "com.etlas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2f0df44e-e5b0-4e95-9134-512e63e63018"
      }
    },
    "scheme": [ "Etlas", "fb473951754912606" ]
  }
}
